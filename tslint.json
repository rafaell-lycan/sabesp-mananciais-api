{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended", "tslint-eslint-rules", "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [true, {
      "multiline": "always",
      "singleline": "never"
    }],
    "quotemark": [true, "single"],
    "indent": [true, "spaces", 2],
    "object-literal-sort-keys": false,
    "member-ordering": false
  },
  "rulesDirectory": []
}
