{
  "name": "sabesp-mananciais-api",
  "version": "2.0.0",
  "description": "A simple scraping from sabesp website",
  "main": "index.js",
  "author": "Rafaell Lycan <sonny.webdsg@gmail.com>",
  "keywords": [
    "sabesp",
    "scraping",
    "mananciais",
    "Ã¡gua"
  ],
  "scripts": {
    "build": "rm -rf build && tsc",
    "develop": "ts-node-dev --respawn --transpileOnly src/server.ts",
    "prod": "yarn build && node ./build/server.js",
    "format": "tslint 'src/**/*.ts' --fix",
    "lint": "tslint 'src/**/*.ts'",
    "start:new": "node build",
    "generate-data": "node build/generator",
    "start": "export DEBUG=sabesp:* && node index.js",
    "pre-test": "./node_modules/jshint/bin/jshint *.js lib/*.js",
    "test:new": "nyc mocha test/**/*.{ts,tsx}",
    "test": "npm run test-node && npm run test-api",
    "test-node": "node_modules/istanbul/lib/cli.js cover node_modules/.bin/_mocha tests/unit/*",
    "test-api": "export DEBUG=sabesp:* && node_modules/istanbul/lib/cli.js cover node_modules/.bin/_mocha tests/api/*",
    "seed": "export DEBUG=sabesp:* && node seed.js"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "@types/js-yaml": "^3.12.1",
    "@types/mocha": "^5.2.6",
    "@types/swagger-tools": "^0.10.6",
    "@types/uuid": "^3.4.4",
    "bluebird": "^3.5.4",
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.3",
    "config": "^3.1.0",
    "cors": "^2.6.0",
    "debug": "^4.1.1",
    "express": "^4.16.4",
    "moment": "^2.10.2",
    "mongodb": "^3.2.4",
    "mongojs": "^2.6.0",
    "nyc": "^14.1.1",
    "request": "^2.55.0",
    "swagger-tools": "^0.10.4",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "universal-analytics": "^0.4.20",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.11",
    "@types/config": "^0.0.34",
    "@types/cors": "^2.8.5",
    "@types/debug": "^4.1.4",
    "@types/express": "^4.16.1",
    "@types/mongodb": "^3.1.27",
    "@types/node": "^12.0.2",
    "@types/request": "^2.48.1",
    "istanbul": "^0.4.5",
    "jshint": "^2.6.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.3.6",
    "prettier": "^1.17.1",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.39",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0"
  },
  "license": "MIT"
}
