{
  "name": "sabesp-mananciais-api",
  "version": "2.0.0",
  "description": "A simple scraping from sabesp website",
  "main": "index.js",
  "author": "Rafaell Lycan <sonny.webdsg@gmail.com>",
  "keywords": [
    "sabesp",
    "scraping",
    "mananciais",
    "Ã¡gua"
  ],
  "scripts": {
    "clean": "rm -rf build",
    "build": "yarn clean && tsc",
    "develop": "export DEBUG=sabesp:* && yarn clean && ts-node-dev --respawn --transpileOnly src/server.ts",
    "prod": "yarn build && node build/server.js",
    "format": "tslint 'src/**/*.ts' --fix && tslint 'test/**/*.ts' --fix",
    "lint": "tslint 'src/**/*.ts' && tslint 'test/**/*.ts'",
    "start:v2": "node build",
    "start": "yarn prod",
    "test": "nyc mocha test/**/*.spec.ts",
    "generate-data": "node build/generator ?seed?",
    "seed": "export DEBUG=sabesp:* && node seed.js"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "dependencies": {
    "bluebird": "^3.5.4",
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.6.0",
    "date-fns": "^1.30.1",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "helmet": "^3.18.0",
    "invariant": "^2.2.4",
    "moment": "^2.10.2",
    "mongodb": "^3.2.4",
    "mongojs": "^2.6.0",
    "morgan": "^1.9.1",
    "node-fetch": "^2.6.0",
    "nyc": "^14.1.1",
    "reflect-metadata": "^0.1.13",
    "request": "^2.55.0",
    "routing-controllers": "^0.7.7",
    "serve-favicon": "^2.5.0",
    "swagger-tools": "^0.10.4",
    "swagger-ui-express": "^4.0.4",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "universal-analytics": "^0.4.20",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cheerio": "^0.22.11",
    "@types/cors": "^2.8.5",
    "@types/date-fns": "^2.6.0",
    "@types/debug": "^4.1.4",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/helmet": "^0.0.43",
    "@types/invariant": "^2.2.29",
    "@types/js-yaml": "^3.12.1",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.27",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.2",
    "@types/node-fetch": "^2.3.4",
    "@types/reflect-metadata": "^0.1.0",
    "@types/request": "^2.48.1",
    "@types/serve-favicon": "^2.2.30",
    "@types/supertest": "^2.0.7",
    "@types/swagger-tools": "^0.10.6",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/universal-analytics": "^0.4.2",
    "@types/uuid": "^3.4.4",
    "@types/winston": "^2.4.4",
    "istanbul": "^0.4.5",
    "jshint": "^2.6.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.3.6",
    "prettier": "^1.17.1",
    "supertest": "^4.0.2",
    "ts-node-dev": "^1.0.0-pre.39",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0"
  },
  "license": "MIT"
}
